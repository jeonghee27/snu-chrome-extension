{
  "name": "SpatialNav",
  "version": "1.0",
  "description": "extension for spatial navigation",
  "manifest_version": 2,
  "devtools_page": "devtool.html",
  "browser_action": {
    "default_icon": "get_started128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["spatnav-heuristic.js", "content_script.js"]
    }
  ],
  "permissions": [
    "activeTab", "tabs", "<all_urls>"
  ]
}